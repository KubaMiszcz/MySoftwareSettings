    "todo-tree.general.tagGroups": {
        "FIX ": [
            "FIXIT ",
            "FIX ",
        ],
        "BUG ": [
            "! ",
            "!! ",
            "!!! ",
            "BUG ",
        ],
        "FUTURE ": [
            "FUT ",
            "FUTURE ",
        ]
    },
    "todo-tree.general.tags": [
        "! ",
        "!! ",
        "!!! ",
        "BUG ",
        "DEPR ",
        "FIX ",
        "FIXIT ",
        "FUT ",
        "FUTURE ",
        "INFO ",
        "TEST ",
        "TODO ",
        "WARN ",
    ],
    "todo-tree.highlights.enabled": true,
    "todo-tree.highlights.highlightDelay": 100,
    "todo-tree.highlights.customHighlight": {
        "! ": {},
        "BUG ": {
            "background": "#A80A0A",
            "foreground": "#FFF5F5",
            "gutterIcon": true,
            "icon": "bug",
            "iconColour": "#c70f0f",
            "rulerColour": "orange",
            "type": "tag-and-comment"
        },
        "DEPR ": {
            "background": "#C1C1C1",
            "foreground": "#171717",
            "fontStyle": "italic",
            "gutterIcon": true,
            "icon": "squirrel",
            "iconColour": "#bbbbbb",
            "rulerColour": "grey",
            "type": "tag-and-comment"
        },
        "FIX ": {
            "background": "#006000",
            "foreground": "#b0ffb0",
            "fontStyle": "bold",
            "gutterIcon": true,
            "icon": "tools",
            "iconColour": "orange",
            "rulerColour": "orange",
            "type": "tag-and-comment"
        },
        "FUTURE ": {
            "background": "#1600BE",
            "foreground": "#E5FFE4",
            "fontStyle": "bold",
            "gutterIcon": true,
            "icon": "rocket",
            "iconColour": "#0080ff",
            "rulerColour": "#0080ff",
            "type": "tag-and-comment"
        },
        "TODO ": {
            "background": "#B02C00",
            "foreground": "#E9EBFF",
            "fontStyle": "bold",
            "gutterIcon": true,
            "icon": "pin",
            "iconColour": "orange",
            "rulerColour": "orange",
            "type": "text-and-comment"
        },
        "TEST ": {
            "background": "#006000",
            "foreground": "#b0ffb0",
            "fontStyle": "bold",
            "gutterIcon": true,
            "icon": "beaker",
            "iconColour": "orange",
            "rulerColour": "orange",
            "type": "tag-and-comment"
        }
    },
    "todo-tree.highlights.defaultHighlight": {
        "fontStyle": "italic",
        "background": "#052b00",
        "foreground": "#09ff00",
        "gutterIcon": true,
        "icon": "info",
        "iconColour": "green",
        "rulerColour": "green",
        "type": "tag-and-comment",
        // "rulerLane": "left"
    },
    "todo-tree.regex.regex": "(//)\\s*($TAGS).*(\\n\\s*//\\s+.*)*",
    // "todo-tree.regex.regex": "(//)\\s*($TAGS)\\s*(.*\\n\\//\\s+.*)*",
    // "todo-tree.regex.regex": "(//)\\s*($TAGS)\\s(.*\\n//\\s+.*)*",
    "todo-tree.regex.regexCaseSensitive": false,
    "todo-tree.tree.expanded": true,
    "todo-tree.tree.flat": true,
    "todo-tree.tree.grouped": true,
    "todo-tree.tree.showScanModeButton": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vsicons.projectDetection.autoReload": true,
