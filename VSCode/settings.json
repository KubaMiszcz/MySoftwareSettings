    "arduino.additionalUrls": [
        "http://arduino.esp8266.com/stable/package_esp8266com_index.json"
    ],
    "bracketPairColorizer.activeScopeCSS": [
        "borderColor : {color}; opacity: 0.5",
        "borderStyle : solid",
        "borderWidth : 1px"
    ],
    // "bracketPairColorizer.rulerPosition": "Left",
    "bracketPairColorizer.scopeLineRelativePosition": true,
    "bracketPairColorizer.showBracketsInGutter": true,
    "bracketPairColorizer.showBracketsInRuler": true,
    "bracketPairColorizer.showHorizontalScopeLine": true,
    "bracketPairColorizer.showVerticalScopeLine": true,
    "breadcrumbs.enabled": true,
    "debug.console.fontSize": 10,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.tslint": true,
        "source.organizeImports": false,
    },
    "editor.columnSelection": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.fontFamily": "'XCourier Prime Code', 'Fira Code Light', Hack, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.formatOnSave": true,
    "editor.matchBrackets": "always",
    "editor.minimap.size": "proportional",
    "editor.padding.bottom": 0,
    "editor.padding.top": 0,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "all",
    "editor.suggestSelection": "first",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "name": "this.self",
                "scope": "variable.language",
                "settings": {
                    "fontStyle": "italic",
                    // "foreground": "#ff5874"
                }
            },
            {
                "scope": "storage.modifier",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "fontStyle": "bold"
                }
            }
        ]
    },
    "editor.wordWrap": "on",
    "emmet.showSuggestionsAsSnippets": true,
    "files.associations": {
        "*.cpp": "cpp",
        "*.json": "jsonc",
        "*.script": "python",
        "*.scss": "scss"
    },
    "files.autoSave": "off",
    "git.autofetch": true,
    "git.confirmSync": false,
    "gitlens.views.repositories.branches.layout": "list",
    "gitlens.views.repositories.files.layout": "tree",
    "html-css-class-completion.enableEmmetSupport": true,
    "html.format.wrapLineLength": 150,
    "htmlhint.options": {
        // "tagname-lowercase": false,
        // "attr-value-double-quotes": true,
